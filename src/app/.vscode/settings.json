{
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "public/build": true
    },
    "eslint.validate": [
        "javascript",
        "svelte"
     ],
     "mochaExplorer.files": "test/**/*.ts",
     "mochaExplorer.require": [ "ts-node/register"],
     "mochaExplorer.env": {
        "TS_NODE_COMPILER_OPTIONS": "{\"module\":\"commonjs\"}"
      },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true
    }
}